<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.catalin.test.ui.main.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment_to_fingerPrintFragment"
            app:destination="@id/fingerPrintFragment" />
        <action
            android:id="@+id/action_mainFragment_to_apiListFragment"
            app:destination="@id/apiListFragment" />
    </fragment>
    <fragment
        android:id="@+id/fingerPrintFragment"
        android:name="com.catalin.test.ui.main.FingerPrintFragment"
        android:label="FingerPrintFragment" />
    <fragment
        android:id="@+id/apiListFragment"
        android:name="com.catalin.test.ui.main.ApiListFragment"
        android:label="ApiListFragment" />
</navigation>